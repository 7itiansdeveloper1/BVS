@model ISas.Entities.Exam_MarksVerificationCoScholasticModels

<div id="headerMainDiv" style="display:none;">
    <h4>
        School Name :- Bal Vikas School, Panipat <br />
        Date        :- @DateTime.Now <br />
        Report Name :- CoSchoolastic Marks Verification  For @Html.DisplayFor(r => Model.SelectedTermName), (@Html.DisplayFor(r => Model.SelectedClassName)-@Html.DisplayFor(r => Model.SelectedSectionName), @Html.DisplayFor(r => Model.SelectedSubjectName))
    </h4>
</div>

@if (Model != null)
{
    <table class="table table-bordered table-responsive table-condensed" id="MarkTable" style="border:1px solid #808080;width:100%;" cellpadding="0" cellspacing="0">
        <thead>
            <tr style="border:1px solid #808080;background-color:#dcce2e;">
                @for (int i = 0; i < Model.HeaderNameList.Count; i++)
                {
                    <th style="border:1px solid #808080;background-color:#dcce2e;">@Html.DisplayFor(r => Model.HeaderNameList[i])</th>
                }
            </tr>
        </thead>
        <tbody>
            @if (Model.ValueList != null && Model.ValueList.Count > 0)
            {
                for (int i = 0; i < Model.ValueList.Count; i++)
                {
                    <tr style="border:1px solid #808080;">
                        @for (int j = 0; j < Model.ValueList[i].Count; j++)
                        {
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.ValueList[i][j]) </td>
                        }
                    </tr>
                }
            }
        </tbody>
    </table>
}
