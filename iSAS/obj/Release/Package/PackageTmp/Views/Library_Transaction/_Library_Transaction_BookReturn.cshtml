@model ISas.Entities.LibraryEntities.Library_TransactionModels

<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12">
        <div class="input-group">
            <span class="input-group-addon">@Html.LabelFor(r => Model.ReturnAccNo)</span>
            @Html.TextBoxFor(m => m.ReturnAccNo, htmlAttributes: new { @class = "form-control", placeholder = "Type/Scan Acc No to Select Return Book", title = "Type/Scan Acc No to Select Return Book" })
            @Html.ValidationMessageFor(r => Model.ReturnAccNo, null, htmlAttributes: new { style = "color:red;" })
        </div>
    </div>
</div>

