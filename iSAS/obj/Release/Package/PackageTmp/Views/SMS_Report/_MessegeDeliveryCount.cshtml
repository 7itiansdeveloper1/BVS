@model List<SelectListItem>

<table id="example2" class="table table-responsive table-bordered table-condensed nowrap" style="width:100%">
    <thead>
        <tr>
            <th>Status</th>
            <th>Count</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>@Html.DisplayFor(r => Model[i].Text)</td>
                    <td>@Html.DisplayFor(r => Model[i].Value)</td>
                </tr>
            }
        }
    </tbody>
</table>

<script>
    $('#example2').DataTable({
        "pageLength": 10,
    });
</script>