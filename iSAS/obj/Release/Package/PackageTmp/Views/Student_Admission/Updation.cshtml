<script type="text/javascript">
        $(document).ready(function () {
            CheckEnableDisableSMSNo();
            //checkToEnableRoute_Stop();
        });

        function checkToEnableRoute_Stop() {
            if ($("input[name='AvailFacilityInfo.Stud_TransReq']:checked").val() == 'True') {
                $('#AvailFacilityInfo_PRouteId').attr('disabled', false);
                $('#AvailFacilityInfo_PStopId').attr('disabled', false);
                getModeOfTransports(true);
            } else {
                //alert('calling');
                $('#AvailFacilityInfo_PRouteId').val(null).change();
                $('#AvailFacilityInfo_PStopId').val(null).change();
                $('#AvailFacilityInfo_PStopId').html('');

                $('#AvailFacilityInfo_PRouteId').attr('disabled', true);
                $('#AvailFacilityInfo_PStopId').attr('disabled', true);
                getModeOfTransports(false);
            }
        }

    function getModeOfTransports(availedTransport) {
        $('#AvailFacilityInfo_ModeofTransport').html('<option value="">--Select Mode--</option>');
        var modeOfTransport = '<option value="">--Select Mode--</option>';
        $.ajax({
            type: "GET",
            url: "../Student_Admission/GetModeOfTransportList",
            data: { AvailedTransport: availedTransport },
            success: function (result) {
                for (var i = 0; i < result.length; i++) {
                    modeOfTransport += '<option value=' + result[i].Value + '>' + result[i].Text + '</option>';
                }
                $('#AvailFacilityInfo_ModeofTransport').html('');
                $('#AvailFacilityInfo_ModeofTransport').html(modeOfTransport);
            },
        });
    }

        function GetSMSNo()
        {
            CheckEnableDisableSMSNo();
            if ($('#AvailFacilityInfo_SMS_WHOM:checked').val() == 'F') {
                $('#AvailFacilityInfo_SMSNo').val($('#FatherInfo_Father_MoblieNo').val());
            }
            else if ($('#AvailFacilityInfo_SMS_WHOM:checked').val() == 'M') {
                $('#AvailFacilityInfo_SMSNo').val($('#MotherInfo_Mother_MobileNo').val());
            }
            else if ($('#AvailFacilityInfo_SMS_WHOM:checked').val() == 'G') {
                $('#AvailFacilityInfo_SMSNo').val($('#GurdianInfo_Gardian_MobileNo').val());
            }
            else if ($('#AvailFacilityInfo_SMS_WHOM:checked').val() == 'O') {
                $('#AvailFacilityInfo_SMSNo').val('');
            }
        }

        function CheckEnableDisableSMSNo() {
            if ($('#AvailFacilityInfo_SMS_WHOM:checked').val() == 'O') {
                $('#AvailFacilityInfo_SMSNo').attr('readonly', false);
            } else {
                $('#AvailFacilityInfo_SMSNo').attr('readonly', true);
            }
        }


        $('.upload').on('change', function (e) {
            var currentImageID = "";
            // var i = $(this).closest('tr').attr('id');
            var forDocType = $(this).closest('div').attr('id');
            if (forDocType == 'S') {
                currentImageID = "selfImg";
            } else if (forDocType == 'F') {
                currentImageID = "fatherImg";
            }
            else if (forDocType == 'M') {
                currentImageID = "motherImg";
            }
            else if (forDocType == 'G') {
                currentImageID = "GardianImg";
            }
            else if (forDocType == 'GF') {
                currentImageID = "GFImg";
            }
            else if (forDocType == 'GM') {
                currentImageID = "GMImg";
            }
            else if (forDocType == 'P') {
                currentImageID = "PImg";
            }

            if (this.files && this.files[0]) {
                var reader = new FileReader();
                reader.onload = function (e) {
                    $('#' + currentImageID)
                        .attr('src', e.target.result)
                        .width(130)
                        .height(150);
                };
                reader.readAsDataURL(this.files[0]);
            }


            var files = e.target.files;
            if (files.length > 0) {
                if (window.FormData !== undefined) {
                    var data = new FormData();
                    for (var x = 0; x < files.length; x++) {

                        data.append("id" + x, files[x]);
                    }
                    data.append('StudentERPNo', $('#OfficeInfo_Stud_UID').val());

                    data.append('ForDocType', forDocType);

                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("UploadFile", "Student_PhotoUpload")',
                    dataType: 'json',
                    contentType: false,
                    processData: false,
                    data: data,
                    success: function (result) {
                        OpenMessegeAutoHideDiv(result.Msg, result.Color );
                    },
                    error: function (xhr, status, p3, p4) {
                        OpenMessegeAutoHideDiv($('#ErrorMsgOnJasonFailed').val(), 'Danger');
                    }
                });
            } else {
                    OpenMessegeAutoHideDiv("This browser doesn't support HTML5 file uploads!", 'Info');
            }
        }
    });
</script>

<script>
    function UpdateSubmitTypeValue(evt) {
        $('#SubmitType').val('BasicForm');
        DoAjaxPostAndMore(evt);
    }
    function SameAsCorres_ChkBoxChange() {
        if ($('#SameAsCorresAddressChkBox').is(':checked')) {
            var stateHtml = '<option value  = "">--Select--</option>';
            stateHtml += '<option selected="selected" value = "' + $('#AddressInfo_Stud_CorrState').val() + '">' + $('#AddressInfo_Stud_CorrState option:selected').text() + '</option>';

            var cityHtml = '<option value = "">--Select--</option>';
            cityHtml += '<option selected="selected" value = "' + $('#AddressInfo_Stud_CorrCity').val() + '">' + $('#AddressInfo_Stud_CorrCity option:selected').text() + '</option>';

            $('#AddressInfo_Stud_PermAdd').val($('#AddressInfo_Stud_CorrAdd').val());
            $('#AddressInfo_Stud_PermPinCode').val($('#AddressInfo_Stud_CorrPinCode').val());
            $('#AddressInfo_Stud_PermMobile').val($('#AddressInfo_Stud_CorrMobile').val());
            $('#AddressInfo_Stud_PermPh1').val($('#AddressInfo_Stud_CorrPhNo1').val());
            $('#AddressInfo_Stud_PermPh2').val($('#AddressInfo_Stud_CorrPhNo2').val());

            $('#AddressInfo_Stud_PermCountry').val($('#AddressInfo_Stud_CorrCountry').val());
            $('#AddressInfo_Stud_PermState').html(stateHtml);
            $('#AddressInfo_Stud_PermCity').html(cityHtml);
            $('#AddressInfo_Stud_PermArea').val($('#AddressInfo_Stud_CorrArea').val()).change();

        }
        else {
            $('#AddressInfo_Stud_PermAdd').val('');
            $('#AddressInfo_Stud_PermPinCode').val('');
            $('#AddressInfo_Stud_PermMobile').val('');
            $('#AddressInfo_Stud_PermPh1').val('');
            $('#AddressInfo_Stud_PermPh2').val('');

            $('#AddressInfo_Stud_PermCountry').val('');
            $('#AddressInfo_Stud_PermState').html('<option value  = "">--Select--</option>');
            $('#AddressInfo_Stud_PermCity').html('<option value  = "">--Select--</option>');
            $('#AddressInfo_Stud_PermArea').val('').change();
        }
    }
    function DoAjaxPostAndMore(btnClicked) {
        var $form = $(btnClicked).parents('form');
        $.ajax({
            type: "POST",
            url: $form.attr('action'),
            data: $form.serialize(),
            error: function (xhr, status, error) {
                OpenMessegeAutoHideDiv($('#ErrorMsgOnJasonFailed').val(), 'Danger');
            },
            success: function (response) {
                if (response.status == 'success') {
                    OpenMessegeAutoHideDiv(response.Msg, response.Color);
                    setTimeout(function () { window.location = "../Student_Admission/Student_AdmissionLandingPage"; }, $('#hddnReloadTime').val());
                }
                else if ($('#SubmitType').val() == 'AdvanceForm' || $('#SubmitType').val() == 'BasicForm') {
                    $('#PanelMainDiv').html(response);
                }
                else {
                    AddErrorAttributeCSS(response.ErrorList); //Available in CustomCommon.js
                    AddValidAttributeCSS(response.ValidKeyList); //Available in CustomCommon.js
                }
            }
        });
    }
</script>

