@model List<ISas.Entities.DashboardEntities.StudentFeeDetailsModel>
<table class="table table-bordered table-responsive table-hover">
    <thead>
        <tr>
            <th>Installment</th>
            <th>Due Date</th>
            <th>Amount</th>
            <th>Recived Amount</th>
            <th>Recived Date</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(r => item.Duration)</td>
                    <td>@Html.DisplayFor(r => item.DueDate)</td>
                    <td>@Html.DisplayFor(r => item.Due)</td>
                    <td>@Html.DisplayFor(r => item.Paid)</td>
                    <td>@Html.DisplayFor(r => item.TransDate)</td>
                    <td>@Html.DisplayFor(r => item.Status)</td>
                </tr>
            }
        }
    </tbody>
</table>
