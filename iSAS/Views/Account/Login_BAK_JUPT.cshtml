<body class="login-page">
    @*<form method="post" action="./sisStudentLogin.aspx?id=sFILXNHjXZY%3d" id="contactForm">*@

        @using (Html.BeginForm())
        {
            @*<div class="aspNetHidden">
                <input type="hidden" name="__VIEWSTATE" id="__VIEWSTATE" value="" />
            </div>

            <div class="aspNetHidden">

                <input type="hidden" name="__VIEWSTATEGENERATOR" id="__VIEWSTATEGENERATOR" value="F9B95AE4" />
                <input type="hidden" name="__EVENTVALIDATION" id="__EVENTVALIDATION" value="/wEdAAkJGG5BNLWUO376+sTej2nppvxcq3PXX/MyWAMHKwZXIWN6ZYJNGAQHn0c9zMgZU3B2NvjHOkq5wKoqN6Aim8WGMKtTyqevv9cjRp4Oh+9VMXDVWoSEvRCzmIlX0qynpXp6TedE/+6yQq+ioVXmajbnJXps88wCOemAvyjg9r39OvtF4jnP78IyOY8aktA+Wp7rbwkesMzod+pqnnQ+5itCX9dbunPN70rUPZ+UwOh5zw==" />
            </div>*@


            <input type="hidden" name="hdnresellerid" id="hdnresellerid" value="36" />

            <div class="container" style="width: 750px; margin: auto;">
                <div class="row">
                    <div class="col-sm-12 text-center">
                        <img src= "~/@ViewBag.HeadDetails.LogoURL"  id="imgSchoolLogo" class="school-logo" />
                    </div>
                </div>
                <div class="studentLogin card">
                    <div class="card-body row">
                        <div class="col-sm-6 padding-border-zero offset-white" style="padding-left: 0;">
                            <div class="left text-center">
                                <p class="pt-5 text-white">
                                    <span id="SchoolNameDisplay">@ViewBag.HeadDetails.Header1</span>

                                </p>
                                <p class="pt-1">
                                    Certainly, there is no purifier in this world like 'knowledge'

                                    <hr /><small>Or call our Parent Helpline : <a href='tel: +91-9891659669' target='_top'>9891659669</a><small>
                                </p>
                                <div class="row mt-5">
                                    <div class="col-sm-6 col-xs-6">
                                        <!--a href="https://goo.gl/atYoOz" target="_blank">
                                        <img src="assets/image/google-play.png" class="img-fluid" />
                                    </a-->
                                    </div>
                                    <div class="col-sm-6 col-xs-6">
                                        <!--a href="https://goo.gl/5z9gYL" target="_blank">
                                        <img src="assets/image/app-store-logo.png" class="img-fluid" />
                                    </a-->
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 offset-white">


                            @using (Html.BeginForm("Login", "Account", FormMethod.Post, htmlAttributes: new { style = "padding:0px 24px;", autocomplete = "off" }))
                            {
                                <div class="login-block">
                                    <div id="LoginDiv">
                                        <div class="login-heading">Login</div>

                                        @Html.ValidationMessage("loginFailedError", htmlAttributes: new { @class = "text-danger" })
                                        
                                        @if (Model != null && Model.SessionList != null)
                                        {
                                         @Html.DropDownListFor(m => m.SelectedSessionId, Model.SessionList, new { @class = "form-control" })
                                        }

                                        <div class="form-group login-input" style="position: relative;">
                                            <i class="fa fa-user overlay"></i>
                                            @*<input name="txtUserName" type="text" maxlength="50" id="txtUserName" class="form-control text-input border-none" placeholder="User Name" name="name" onkeypress="return OnKeyPress(event);" />*@
                                            @Html.TextBoxFor(m => m.username, new { @class = "form-control", autocomplete = "off", placeholder = "User Name" })
                                        </div>


                                        <div class="form-group login-input ">
                                            <i class="fa fa-key overlay"></i>
                                            @*<input name="txtPassword" type="password" maxlength="25" id="txtPassword" class="form-control text-input border-none" placeholder="********" onkeypress="return OnKeyPress(event);" />*@
                                            @Html.PasswordFor(m => m.password, new { @class = "form-control", autocomplete = "off", placeholder = "Password" })
                                        </div>


                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div class="checkbox">
                                                    <label>
                                                        <input id="chkRemember" type="checkbox" name="chkRemember" />Remember me
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div class="form-group login-input no-border">
                                                    <small id="smErrorMsg" class="help-block" style="display:none;"></small>
                                                    <small id="smNoOfAttempts" class="help-block" style="color:#df5d36;display:none;"></small>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row mt-5">
                                            <div class="col-sm-6 col-xs-6">
                                                @*<input type="submit" name="btn_Login" value="LOGIN" onclick="return validateLogin();" id="btn_Login" class="btn btn-success btn-md" />
                                                <input type="submit" name="btn_Loging" value="LOGING" id="btn_Loging" class="btn btn-success" style="display: none;" />*@
                                                @*<button class="btn btn-success btn-md" type="submit" >Login</button>*@
                                                <button type="submit" class="bttn">Login</button>
                                            </div>
                                            <div class="col-sm-6 reset col-xs-6">
                                                <a href="javascript:void(0);" class="btn btn-danger btn-md" onclick="reset();">Reset</a>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12">
                                            </div>
                                            <div id="OTPprocedeDiv" style="display:none;">
                                                <div class="alert alert-success">
                                                    <h2>Enter One Time Password (OTP)</h2>
                                                </div>
                                                <div id="OTPErrorMsg" class="alert alert-info">
                                                    One Time Password (OTP) has been sent to your mobile ******<span id="mobileLastDigit"></span> please enter the same here to login.
                                                </div>
                                                <div class="form-group login-input">
                                                    <i class="fa fa-user overlay"></i>
                                                    <input name="txtOTP" type="text" id="txtOTP" placeholder="Enter OTP" class="form-control text-input" onkeypress="return clickProcedure(event);" />
                                                    <br />
                                                    <small id="mobileRegisteredMsg" class="help-block" style="color:#96444E;display:none;"></small>
                                                </div>
                                                <div class="row">
                                                    <div class="col-sm-6 reset">
                                                        <a href="" class="btn btn-default btn-block">Cancel</a>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <input type="submit" name="btnProceed" value="PROCEED" onclick="return validateProcede();" id="btnProceed" class="btn btn-success btn-block" />

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            }
                        </div>
                    </div>
                </div>
                <p class="text-center"><a href="sisForgetPasswordForStudent.aspx?id=sFILXNHjXZY=" id="forgetPassword" class="help-label">Forget Password?</a></p>
            </div>
            <div class="Footer-div-img">
                <br />
                <img id="ImgProd" title="eConnect-K12" class="econnect-logo" src="Images/7itians_logo.png" alt="eConnect-K12" style="max-width: 150px;" />
            </div>
            <div class="Footer-div">
                <br />
                <br />
                <br />
                <br />
                <p style="text-align: center; font-size: 10px; letter-spacing: 1px;">All Right Reserved to Bal Vikas School, Panipat. Powered By : <a id="hrefpow" class="footer-jupsoft" target="_blank">7itians</a></p>
            </div>
        }

        @*</form>*@
</body>





</html>

