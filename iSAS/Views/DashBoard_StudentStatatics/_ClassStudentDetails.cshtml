@model List<ISas.Entities.DashboardEntities.StudentDetailsModel>

<table id="example1" class="table table-responsive table-bordered table-condensed display nowrap" style="width:100%;">
    <thead>
        <tr>
            <th>Adm No</th>
            <th>Student</th>
            <th>Father</th>
            <th>Mother</th>
            <th>Mobile</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@Html.DisplayFor(r => Model[i].AdmNo)</td>
                <td>@Html.DisplayFor(r => Model[i].Student)</td>
                <td>@Html.DisplayFor(r => Model[i].Father)</td>
                <td>@Html.DisplayFor(r => Model[i].Mother)</td>
                <td>@Html.DisplayFor(r => Model[i].SMSNo)</td>
            </tr>
        }
    </tbody>
</table>

<script>
    $('#example1').DataTable({});
</script>