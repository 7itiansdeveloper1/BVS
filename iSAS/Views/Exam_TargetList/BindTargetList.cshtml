@model ISas.Entities.Exam_Entities.Exam_TargetModels

<div id="marksVeriHeader" style="display:none;">
    <h4>
        School Name :- Bal Vikas School, Panipat <br />
        Date        :- @DateTime.Now <br />
        Report Name :- Target List For (@Html.DisplayFor(r => Model.ClassName), @Html.DisplayFor(r => Model.SubjectName))
    </h4>
</div>

<div class="col-xs-12">
    <div class="table-responsive searchtable table_ev_od" style="min-height:400px;">
        <table class="table table-bordered table-responsive" id="MarkTable" style="border:1px solid #808080;width:100%;" cellpadding="0" cellspacing="0">
            <thead>
                <tr style="border:1px solid #808080;background-color:#dcce2e;">
                    <td>ERPNO</td>
                    <td>Student</td>
                    <td>UT</td>
                    <td>HY</td>
                    <td>UTBEST</td>
                    <td>Final</td>
                    <td>Total</td>
                    <td>Passing Mark</td>
                    <td>Target</td>
                </tr>
            </thead>
            <tbody>
                @if (Model.TargetList != null && Model.TargetList.Count > 0)
                {

                    for (int i = 0; i < Model.TargetList.Count; i++)
                    {
                        <tr style="border:1px solid #808080;">
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.TargetList[i].ERPNO) </td>
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.TargetList[i].Student) </td>
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.TargetList[i].UT) </td>
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.TargetList[i].HY) </td>
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.TargetList[i].UTBEST) </td>
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.TargetList[i].Final) </td>
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.TargetList[i].Total) </td>
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.TargetList[i].PassingMark) </td>
                            <td style="border:1px solid #808080;text-align:center;"> @Html.DisplayFor(r => Model.TargetList[i].Target) </td>
                        </tr>
                    }


                }
            </tbody>
        </table>
    </div>
</div>

