@model List<ISas.Entities.LibraryEntities.Library_FineSetupModels>

<table id="example1" class="table table-responsive table-bordered table-condensed display nowrap" style="width:100%;">
    <thead>
        <tr>
            <th>#</th>
            <th>Fine For Staff</th>
            <th>Fine For Student</th>
            <th>Is Active</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @{ int count = 1;
            for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>@Html.DisplayFor(r => count)</td>
                    <td>@Html.DisplayFor(r => Model[i].FineRateForStaff)</td>
                    <td>@Html.DisplayFor(r => Model[i].FineRateForStudent)</td>
                    <td>@Html.DisplayFor(r => Model[i].IsActive)</td>
                    <td><a href="#" title="Edit Fine" class="fa fa-edit btnDefaultColor" onclick="EditFineSetup('@Model[i].FineId')"></a></td>
                </tr>
                count++;
            }
        }
    </tbody>
</table>

<script>
    $('#example1').DataTable({});
</script>